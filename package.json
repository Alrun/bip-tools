    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/**/*.d.ts",
      "!src/**/*Styles.ts"
    ],
    "coverageThreshold": {
      "global": 30
    }
  },
  "overrides": {
    "@types/react": "17.0.44"
  },
  "resolutions": {
    "@types/react": "17.0.44"
  }
